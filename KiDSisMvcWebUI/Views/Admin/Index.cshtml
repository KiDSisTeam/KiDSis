@model IEnumerable<KiDSisMvcWebUI.Identity.ApplicationUser>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout-Login.cshtml";
}
<div class="container">


    <h2>Index</h2>

    <div class="panel panel-primary">
        <div class="panel-headding">
            users
        </div>
        <table class="table table-striped">
            <tr>
                <th>Id</th>
                <th>User Name</th>
                <th>Email</th>
            </tr>
            @if (Model.Count() == 0)
            {
                <tr>
                    <td colspan="3" class="text-center">
                        Kullanıcı yok
                    </td>
                </tr>
            }
            else
            {
                foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.UserName</td>
                        <td>@item.Email</td>


                    </tr>

                }
            }
        </table>

    </div>
</div>